<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.5+</version>
	</compatibility>
	<theme id="training" name="training-theme" >
		<settings>
			<setting key="portlet-setup-show-borders-default" value="false" />
			<setting configurable="true" key="show-menu" value="true" type="checkbox" />
			<setting configurable="true" key="show-footer" value="true" type="checkbox" />
			<setting configurable="true" key="show-header" value="true" type="checkbox" />
			<setting configurable="true" key="use-header-profil" value="false" type="checkbox" />
			<setting configurable="true" key="show-background" value="false" type="checkbox" />
		</settings>
	</theme>
</look-and-feel>