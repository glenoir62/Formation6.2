<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder auto-namespace-tables="false" package-path="fr.smile.training.faq">
    <author>Guillaume Lenoir</author>
    <namespace>FAQ</namespace>
    <entity name="Faq" local-service="true">
    	<column name="entryId" type="long" primary="true"></column>
    	<column name="companyId" type="long"></column>
    	<column name="groupId" type="long"></column>
    	<column name="question" type="String"></column>
    	<column name="answer" type="String"></column>
        <reference entity="Counter" package-path="com.liferay.counter"></reference>
    </entity>
	
</service-builder>